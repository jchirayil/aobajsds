{
  "name": "aobajsds",
  "version": "1.0.0",
  "description": "A TypeScript library to manage JSON as a dataset.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "prepublishOnly": "npm run build && npm lint",
    "test": "mocha -r ts-node/register \"test/**/*.test.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "docs": "typedoc --out docs src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jchirayil/aobajsds.git"
  },
  "keywords": [
    "json",
    "dataset",
    "table",
    "typescript",
    "index",
    "sort",
    "filter",
    "query"
  ],
  "author": "Jacob Chirayil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jchirayil/aobajsds/issues"
  },
  "homepage": "https://github.com/jchirayil/aobajsds#readme",
  "devDependencies": {
    "@types/chai": "^5.2.1",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "chai": "^5.2.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "jszip": "^3.10.1",
    "mocha": "^11.1.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.3",
    "typedoc-plugin-markdown": "^4.6.2",
    "typescript": "^5.8.3"
  }
}
